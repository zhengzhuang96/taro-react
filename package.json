{
  "name": "taro-react-template",
  "version": "1.0.0",
  "private": true,
  "description": "taro-react-template",
  "templateInfo": {
    "name": "redux",
    "typescript": false,
    "css": "less"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "test:weapp": "cross-env NODE_ENV=testEnv npm run build:weapp",
    "eslint": "eslint --ext .js,.jsx src",
    "eslintfix": "eslint --fix --ext .js,.jsx src",
    "prepare": "husky install",
    "lint": "eslint",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --config commitlint.config.js -e -V"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@rematch/core": "^2.2.0",
    "@rematch/loading": "^2.1.2",
    "@tarojs/components": "3.4.10",
    "@tarojs/plugin-framework-react": "3.4.10",
    "@tarojs/plugin-platform-weapp": "^3.4.13",
    "@tarojs/react": "3.4.10",
    "@tarojs/runtime": "3.4.10",
    "@tarojs/taro": "3.4.10",
    "axios": "^0.27.2",
    "js-base64": "^3.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-ui": "^3.1.0-beta.2",
    "taro-weapp-poster": "^1.3.0",
    "tree": "^0.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@tarojs/mini-runner": "3.4.10",
    "@tarojs/webpack-runner": "3.4.10",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "3.4.10",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-taro": "3.4.10",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "mddir": "^1.1.1",
    "prettier": "2.7.1",
    "sass-migrator": "^1.5.6",
    "stylelint": "9.3.0"
  },
  "resolutions": {
    "@types/react": "^17.0.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "src/*": "eslint --ext .js,.jsx"
  }
}